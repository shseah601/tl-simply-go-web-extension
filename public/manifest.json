{
  "manifest_version": 3,
  "name": "Simply Go Extension",
  "version": "1.0.1.0",
  "version_name": "1.0.1.0 build 20221205-1",
  "description": "Extra functionalities for Simply Go",
  "action": {
    "default_icon": {
      "16": "logo/logo-16.png",
      "32": "logo/logo-32.png"
    },
    "default_title": "SimplyGo Extra",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo/logo-16.png",
    "32": "logo/logo-32.png",
    "48": "logo/logo-48.png",
    "128": "logo/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://simplygo.transitlink.com.sg/*"],
      "js": ["static/js/content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "lib/*", "static/js/content.js.map" ],
      "matches": [ "https://simplygo.transitlink.com.sg/*" ]
    }
  ]
}